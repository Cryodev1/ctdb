    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.2.1",
    "fastify": "^4.4.0",
    "graphql": "^16.1.0",
    "graphql-helix": "^1.11.0",
    "graphql-import-node": "^0.0.4",
    "graphql-scalars": "^1.14.1",
    "lodash": "^4.17.21",
